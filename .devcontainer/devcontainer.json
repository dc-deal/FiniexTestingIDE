{
    "name": "FiniexTestingIDE Dev",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "finiex-dev",
    "workspaceFolder": "/app",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.debugpy",
                "ms-toolsai.jupyter"
            ],
            "settings": {
                "python.defaultInterpreterPath": "/usr/local/bin/python",
                "python.terminal.activateEnvironment": false
            }
        }
    },
    "forwardPorts": [
        8888
    ],
    //"postCreateCommand": "pip install -e .",
    "remoteUser": "root"
}